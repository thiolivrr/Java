public class Boletim {
	double notaNP1;
	double notaNP2;
	
	public Boletim(double aNotaNP1, double aNotaNP2) {
		notaNP1 = aNotaNP1;
		notaNP1 = aNotaNP2;
	}
	
	public double media() {
		return (this.notaNP1 + this.notaNP2)/2.0;
	}
	
	boolean passou() {
		if ((notaNP1 + notaNP2) / 2  > 7 ) {
			return true;
		} else {
			return false;
		}
	}
}
