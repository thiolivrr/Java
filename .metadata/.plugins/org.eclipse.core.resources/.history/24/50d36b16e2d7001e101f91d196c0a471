
public class Main {

	public static void main(String[] args) {
		Pato pato = new Pato("Junior", 9);
		pato.voa();
		pato.nada();
		pato.quack();
		PatoMadeira patoMadeira = new PatoMadeira("Junior", 9);
		patoMadeira.voa();
		patoMadeira.nada();
		patoMadeira.quack();
		PatoBorracha PatoBorracha = new PatoBorracha("Junior", 9, "Azul");
		PatoBorracha.voa();
		PatoBorracha.nada();
		PatoBorracha.quack();

	}

}
