
public class Pato {
	String nome;
	int ano;
	
	public Pato(String aNome, int aAno) {
		nome = aNome;
		ano = aAno;
	}

	public void voa() {
		System.out.println("O pato voa");
	}
	
	public void nada() {
		System.out.println("O pato nada");
	}
	
	public void quack() {
		System.out.println("Quack");
	}
}
