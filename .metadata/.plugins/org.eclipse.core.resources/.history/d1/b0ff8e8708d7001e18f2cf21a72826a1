
public class Aluno {
	String ra;
	String nome;
	double notaNP1;
	double notaNP2;
	
	public Aluno(String aRa, String aNome, double aNotaNP1, double aNotaNP2) {
		ra = aRa;
		nome = aNome;
		notaNP1 = aNotaNP1;
		notaNP2 = aNotaNP2;
	}
	
	
	public void relatorio() {
		System.out.println(this);
	}
	
	@Override
	public String toString() {
		String res = "";
		res += "Nome: " + this.nome + "\n";
		res += "RA: " + this.ra + "\n";
		res += "NotaNP1: " + this.notaNP1 + "\n";
		res += "NotaNP2: " + this.notaNP2;
		return res;
	}
}

